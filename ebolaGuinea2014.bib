	File = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/tit72ymn.default/zotero/storage/VQXTZ4NR/Carroll et al. - 2013 - Molecular Evolution of Viruses of the Family Filov.pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/tit72ymn.default/zotero/storage/ZN3IRKQH/2608.html:text/html},
	Issn = {0022-{538X}, 1098-5514},
